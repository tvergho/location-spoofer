{
  "name": "location-spoofer",
  "productName": "Location Spoofer",
  "version": "1.0.0",
  "description": "App to spoof an iPhone's location.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "Tyler Vergho",
    "email": "tvergho@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "osxSign": {
          "hardened-runtime": true,
          "entitlements": "entitlements.mac.plist",
          "entitlements-inherit": "entitlements.mac.plist",
          "signature-flags": "library",
          "gatekeeper-assess": false
        },
        "osxNotarize": {
          "appleId": "tvergho@gmail.com",
          "appleIdPassword": "fbca-athz-zefu-ucht",
          "ascProvider": "KevinVergho1394596107"
        },
        "extraResource": [
          "./libimobiledevice"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-dmg",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "build": {
    "appId": "com.electron.location-spoofer",
    "productName": "Location Spoofer",
    "copyright": "Copyright Â© 2021 Tyler Vergho",
    "mac": {
      "target": "dmg",
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "directories": {
      "buildResources": "build"
    },
    "extraResources": [
      "./libimobiledevice"
    ],
    "dmg": {
      "sign": false
    },
    "afterSign": "./notarize.js"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.59",
    "@electron-forge/maker-deb": "^6.0.0-beta.59",
    "@electron-forge/maker-dmg": "^6.0.0-beta.59",
    "@electron-forge/maker-pkg": "^6.0.0-beta.59",
    "@electron-forge/maker-rpm": "^6.0.0-beta.59",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.59",
    "@electron-forge/maker-zip": "^6.0.0-beta.59",
    "@electron-forge/plugin-webpack": "6.0.0-beta.59",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@vercel/webpack-asset-relocator-loader": "1.6.0",
    "css-loader": "^6.0.0",
    "electron": "13.1.8",
    "electron-builder": "^22.11.7",
    "electron-notarize": "^1.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fork-ts-checker-webpack-plugin": "^6.0.1",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "electron-log": "^4.4.1",
    "electron-root-path": "^1.0.16",
    "electron-squirrel-startup": "^1.0.0",
    "fix-path": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browser": {
    "child_process": false
  }
}
